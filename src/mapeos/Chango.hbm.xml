<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="datos.Chango" table="chango">
    <id column="idChango" name="idChango">
        <generator class="identity"/>
    </id>
    <set name="listaItems" table="item" order-by="idItem asc" inverse="true" lazy="true" fetch="select">
      <key column="idChango" not-null="true"/>
      <one-to-many class="datos.Item"/>
    </set>

    <one-to-one name="factura" class="datos.Factura"></one-to-one>
    <one-to-one name="pedidostock" class="datos.PedidoStock"></one-to-one>

    <many-to-one name="local" class="datos.Local" column="idLocal" not-null="true"/>
  </class>
</hibernate-mapping>
