<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="datos.PedidoStock" table="pedidostock">
    <id column="idPedido" name="idPedido" >
        <generator class="identity"/>
    </id>
    <property column="cantidad" name="cantidad" type="int"/>
    <property column="aceptado" name="aceptado" type="boolean"/>

    <one-to-one name="producto" class="datos.Producto"></one-to-one>
    <one-to-one name="solicitante" class="datos.Empleado"></one-to-one>
    <one-to-one name="oferente" class="datos.Empleado"></one-to-one>
    <one-to-one name="chango" class="datos.Chango"></one-to-one>
  </class>
</hibernate-mapping>
